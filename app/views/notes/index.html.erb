<h1 class="display-1">All Notes</h1>
<% @notes.each do |note| %>
    <h2 class="display-5"><%= link_to note.title, note %></h2>
    <h3 class="display-6"><%= time_ago_in_words(note.created_at) %> ago</h3>
    <!-- <h3>Created By: <%= note.user_id %></h3> -->
    <p><%= note.body %></p>
    <strong><%= link_to "Delete", note_path(note), method: :delete %></strong>
    <strong><%= link_to "Edit", edit_note_path(note) %></strong>
    <br/><br/>
<% end %>